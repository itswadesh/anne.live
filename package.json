{
  "name": "litekart",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "run-s anne-copy-config nuxt",
    "nuxt": "nuxt",
    "build": "nuxt build",
    "builda": "nuxt build -a",
    "u": "git pull upstream master",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && rm -rf public/build && npm install && npm run build:tailwind && npm run build:fontawesome && npm run dev",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:fix": "npm run lint -- --fix",
    "prettify": "prettier --write \"**/*.{js,jsx,vue,json,css,scss,md}\"",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "copy": "node devops/copy",
    "anne-copy-config": "node devops/anne/copy-config",
    "anne-copy": "node devops/anne/copy",
    "anne-deploy": "node devops/anne/deploy",
    "anne": "run-s anne-copy-config build anne-copy anne-deploy",
    "anne-deploy-live": "node devops/anne/live",
    "live-anne": "run-s anne-copy-config build anne-copy anne-deploy-live",
    "live": "run-s live-anne",
    "lived": "run-s docker-build docker-push",
    "docker-build": "docker build -t itswadesh/tablez-fontend .",
    "docker-push": "docker push itswadesh/tablez-fontend",
    "pull": "git submodule update --init --recursive"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint",
      "prettier --write",
      "git add"
    ],
    "*.{css,vue}": [
      "stylelint",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@bachdgvn/vue-otp-input": "^1.0.8",
    "@nuxtjs/apollo": "^4.0.1-rc.4",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/firebase": "^7.6.1",
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/proxy": "^2.0.1",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/robots": "^2.5.0",
    "@nuxtjs/tailwindcss": "^4.2.1",
    "@nuxtjs/toast": "^3.3.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.0",
    "cookie-universal-nuxt": "^2.1.5",
    "core-js": "^3.15.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "firebase": "^8.7.1",
    "flv.js": "^1.5.0",
    "hls.js": "^1.0.10",
    "is-https": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^2.15.8",
    "nuxt-i18n": "^6.27.2",
    "nuxt-webfontloader": "^1.1.0",
    "push.js": "^1.0.12",
    "shortcode-tokenizer": "^0.6.1",
    "tailwindcss-rtl": "^0.7.0",
    "vue-ctk-date-time-picker": "^2.5.0",
    "vue-gtag": "^1.16.1",
    "vue-infinite-loading": "^2.4.5",
    "vue-lazyload": "^1.3.3",
    "vue-plain-pagination": "^0.3.0",
    "vue-ripple-directive": "^2.0.1",
    "vue-social-sharing": "^3.0.8",
    "vue-sweetalert2": "^5.0.2",
    "vue-toasted": "^1.1.28",
    "vue-youtube": "^1.4.0",
    "vuejs-countdown": "^0.2.1",
    "vuelidate": "^0.7.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@nuxt/image": "^0.6.0",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@tailwindcss/line-clamp": "^0.2.1",
    "autoprefixer": "^10.3.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "nuxt-purgecss": "^1.0.0",
    "posix-character-classes": "^1.0.0",
    "postcss": "^8.3.8",
    "prettier": "^2.4.1",
    "shelljs": "^0.8.4",
    "ssh2shell": "^1.9.5",
    "stylelint": "^13.12.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "yarn": "^1.22.11"
  }
}